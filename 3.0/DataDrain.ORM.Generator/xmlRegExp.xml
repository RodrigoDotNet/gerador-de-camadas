<tipos>
  <tipo>
    <nome>Apenas números [0 á 9]</nome>
    <expression>^[0-9]+$</expression>
  </tipo>
  <tipo>
    <nome>Apenas letras [A-Z]</nome>
    <expression>^[a-zA-Z]+$</expression>
  </tipo>
  <tipo>
    <nome>Alfanumérico [A-Z e 0 á 9]</nome>
    <expression>^[a-zA-Z0-9]+$</expression>
  </tipo>
  <tipo>
    <nome>Numérico [0 á 9]</nome>
    <expression>^[0-9]+$</expression>
  </tipo>
  <tipo>
    <nome>Telefone - [(99)9999-9999]</nome>
    <expression>^\(\d{2}\)\d{4}-\d{4}$</expression>
  </tipo>
  <tipo>
    <nome>CEP - [99.999-999]</nome>
    <expression>^\d{2}.\d{3}-\d{3}$</expression>
  </tipo>
  <tipo>
    <nome>E-mail</nome>
    <expression>\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*</expression>
  </tipo>
  <tipo>
    <nome>Url</nome>
    <expression><![CDATA[http(s)?://([\w-]+\.)+[\w-]+(/[\w- ./?%&=]*)?]]></expression>
  </tipo>
  <tipo>
    <nome>IP</nome>
    <expression>^([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(\.([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])){3}$</expression>
  </tipo>
  <tipo>
    <nome>Caminho Fisico</nome>
    <expression><![CDATA[^(([a-zA-Z]\:)|(\\))(\\{1}|((\\{1})[^\\]([^/:*?<>""|]*))+)$]]></expression>
  </tipo>
  <tipo>
    <nome>Arquivos de Imagem [ jpg|jpeg|png|gif ]</nome>
    <expression>^.*\.(jpg|JPG|jpeg|JPEG|png|PNG|gif|GIF)$</expression>
  </tipo>
  <tipo>
    <nome>Arquivos de Documentos [ Word|Pdf|Excel ]</nome>
    <expression>^.*\.(doc|DOC|docx|DOCX|pdf|PDF|xls|XLS|xlsx|XLSX)$</expression>
  </tipo>
  <tipo>
    <nome>Arquivos Video [ flv|f4v|avi ]</nome>
    <expression>^.*\.(flv|FLV|f4v|F4V|avi|AVI)$</expression>
  </tipo>
  <tipo>
    <nome>Senha segura</nome>
    <expression><![CDATA[^.*(?=.{6,})(?=.*\d)(?=.*[a-z])(?=.*[A-Z])(?=.*[@#$%^&+=]).*$]]></expression>
  </tipo>
  <tipo>
    <nome>GUID</nome>
    <expression>^(\{){0,1}[0-9a-fA-F]{8}\-[0-9a-fA-F]{4}\-[0-9a-fA-F]{4}\-[0-9a-fA-F]{4}\-[0-9a-fA-F]{12}(\}){0,1}$</expression>
  </tipo>
  <tipo>
    <nome>String como Data</nome>
    <expression><![CDATA[^([1-9]|0[1-9]|[12][0-9]|3[01])[- /.]([1-9]|0[1-9]|1[012])[- /.][0-9]{4}$]]></expression>
  </tipo>
  <tipo>
    <nome>String sem espaço</nome>
    <expression>^[\S]+$</expression>
  </tipo>
  <tipo>
    <nome>Valida Hexadecimal</nome>
    <expression>^([A-Fa-f0-9]{2}){8,9}$</expression>
  </tipo>
  <tipo>
    <nome>Valida Hash MD5</nome>
    <expression>^[0-9a-f]{32}$</expression>
  </tipo>
  <tipo>
    <nome>Valida Hash SHA1</nome>
    <expression>^[0-9a-f]{40}$</expression>
  </tipo>
